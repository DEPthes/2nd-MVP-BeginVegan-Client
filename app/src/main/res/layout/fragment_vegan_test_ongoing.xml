<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".src.ui.view.test.VeganTestOngoingFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ProgressBar
            android:id="@+id/pb_vegan_test_ongoing"
            style="?android:attr/progressBarStyleHorizontal"
            android:max="5"
            android:progressBackgroundTint="@color/color_gray_e9"
            android:progressTint="@color/color_primary3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/margin_100"
            android:layout_marginHorizontal="@dimen/side_margin"/>
        <FrameLayout
            android:id="@+id/fl_test_questions"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/pb_vegan_test_ongoing"
            android:layout_marginTop="@dimen/margin_40"/>
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>